# if
snippet if
  if (${1:true}) {
    ${2}
  }

# switch
snippet switch
  switch (${1:expression}) {
    case '${3:case}':
      ${4:// code}
      break;
    ${5}
    default:
      ${2:// code}
  }

# for (...) {...}
