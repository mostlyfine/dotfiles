  for (var ${2:i} = 0; $2 < ${1:Things}.length; $2${3: += 1}) {
    ${4:$1[$2]}
  }

# while (...) {...}
snippet while
  while (${1:/* condition */}) {
    ${2:/* code */}
  }

# try
snippet try
  try {
    ${1:/* code */}
  } catch (${2:e}) {
    ${3:/* handle error */}
  }

# do...while
snippet do
  do {
    ${2:/* code */}
